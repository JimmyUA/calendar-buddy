# Telegram Bot Token
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Google OAuth Client Secrets (either content or file path)
GOOGLE_CLIENT_SECRETS_CONTENT='your_google_client_secrets_json_content'
# GOOGLE_CLIENT_SECRETS_FILE=/path/to/your/client_secret.json

# Google API Key for Gemini
GOOGLE_API_KEY=your_google_api_key

# OAuth Redirect URI
OAUTH_REDIRECT_URI=https://your-app-url.com/oauth2callback

# Web Server Settings
WEB_SERVER_HOST=127.0.0.1
WEB_SERVER_PORT=5000

# Gemini Model Name
# Replace with a valid and available Gemini model name
# Example: gemini-1.5-flash-001
GEMINI_MODEL=gemini-1.5-flash-001
